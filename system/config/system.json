{
  "version": "1.0.0",
  "name": "Bio-Adaptive Haptic Coaching System",
  "environment": "development",
  "timestamp": "2026-01-17T00:00:00Z",
  "components": {
    "biometrics": {
      "enabled": true,
      "sampling_rate": 250,
      "signals": [
        "ecg",
        "hr",
        "hrv"
      ]
    },
    "reinforcement_learning": {
      "enabled": true,
      "algorithm": "PPO",
      "episodes": 50,
      "learning_rate": 0.0002,
      "batch_size": 128,
      "gamma": 0.995
    },
    "simulation": {
      "enabled": true,
      "environment": "MotorcycleEnv",
      "max_velocity": 200,
      "timesteps": 600
    },
    "adversarial_training": {
      "enabled": true,
      "noise_levels": 100,
      "max_noise_scale": 0.7
    },
    "safety": {
      "bio_gating": true,
      "stress_threshold": 0.6,
      "activation_mode": "adaptive"
    }
  },
  "deployment": {
    "target": "local",
    "quantization": "fp32",
    "timeout": 30,
    "monitoring": true,
    "auto_rollback": true
  },
  "visualization": {
    "dpi": 300,
    "format": "png",
    "interactive": true,
    "server_port": 7860,
    "theme": "dark",
    "mode": "gradio"
  },
  "paths": {
    "data": "workspace/data",
    "models": "workspace/models",
    "results": "workspace/results",
    "logs": "workspace/logs",
    "experiments": "workspace/experiments"
  },
  "performance": {
    "target_latency_ms": 140,
    "target_accuracy": 0.9,
    "min_robustness": 0.88,
    "min_safety": 0.93
  }
}